{
    "compilerOptions": {
        "baseUrl": "src",
        "rootDir": "src",
        "outDir": "lib",
        "target": "es5",
        "lib": [
            "es2015"
        ]
    },
    "exclude": [
        "node_modules",
        "test",
        "lib"
    ],
    "packOptions": {
        "emitCustomHandlers": true,
        "wrapOutput": "(function(){%output%})();",
        "excludeImports": [
            "bluebird",
            "builtin-modules",
            "magic-string",
            "node-getopt",
            "source-map",
            "typescript"
        ]
    }
}